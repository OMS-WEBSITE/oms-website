---
import Layout from '../layouts/main.astro';
import AuditProApp from '../components/AuditProApp';

// SEO metadata specific to homepage
const seo = {
	title: "Audit Pro - Modern Security Auditing & Compliance Platform",
	description: "Transform your security auditing process with AI-powered assessment tools, automated compliance checks, and comprehensive quality management. Built with Astro, React, and TypeScript for optimal performance.",
	image: "/og-image.png",
	canonicalURL: Astro.site + Astro.url.pathname,
};
---

<Layout {...seo}>
	<AuditProApp client:load />
</Layout>
